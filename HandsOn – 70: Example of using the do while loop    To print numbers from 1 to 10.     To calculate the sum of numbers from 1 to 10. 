public class DoWhileExample { 

    public static void main(String[] args) { 

        // Using do-while loop to print numbers from 1 to 10 

        System.out.println("Using do-while loop to print numbers from 1 to 10:"); 

        int i = 1; 

        do { 

            System.out.println(i); 

            i++; 

        } while (i <= 10);        

    int sum = 0; 

        int j = 1; 

do{ 

                sum=sum+j; 

        System.out.println("Sum of numbers till "+ j+" : "+ sum); 

                j++; 

} while (j <= 10); 

   } 

   } 
