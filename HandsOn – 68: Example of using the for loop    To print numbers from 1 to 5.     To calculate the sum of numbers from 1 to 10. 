public class ForLoopExample { 

    public static void main(String[] args) { 

 

        // Using for loop to print numbers from 1 to 5 

        System.out.println("Using for loop:"); 

        for (int i = 1; i <= 5; i++) { 

            System.out.println(i); 

        } 

 

 

        // Using for loop to calculate the sum of numbers from 1 to 10 

        int sum = 0; 

        int j; 

        for (j = 1; j <= 10; j++) { 

            sum += j; 

        } 

        System.out.println("\n Sum of numbers from 1 to 10: " + sum); 

    } 

} 

 
