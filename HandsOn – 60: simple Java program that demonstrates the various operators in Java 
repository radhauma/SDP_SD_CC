public class OperatorsExample { 

    public static void main(String[] args) { 

        // Arithmetic Operators 

        int a = 10; 

        int b = 5; 

       int sum = a + b; 

        int difference = a - b; 

        int product = a * b; 

        int quotient = a / b; 

        int remainder = a % b; 

        System.out.println("Arithmetic Operators:"); 

        System.out.println("Sum: " + sum); 

        System.out.println("Difference: " + difference); 

        System.out.println("Product: " + product); 

        System.out.println("Quotient: " + quotient); 

        System.out.println("Remainder: " + remainder); 

        System.out.println(); 

        // Assignment Operators 

        int x = 20; 

        int y = 20; 

        int z = 20; 

        int w = 20; 

        x += 5; // Equivalent to x = x + 5 

        y -= 5; 

        z /= 5; 

        w %= 5; 

        System.out.println("Assignment Operator (+=): "+x); 

        System.out.println("Assignment Operator (-=): "+y); 

        System.out.println("Assignment Operator (/=): "+z); 

        System.out.println("Assignment Operator (%=): "+w); 

        System.out.println(); 

        // Relational Operators 

        boolean isEqual = (a == b); 

        boolean isGreater = (a > b); 

        boolean isLessOrEqual = (a <= b); 

        System.out.println("Relational Operators:"); 

        System.out.println("Is Equal: " + isEqual); 

        System.out.println("Is Greater: " + isGreater); 

        System.out.println("Is Less or Equal: " + isLessOrEqual); 

        System.out.println(); 

        // Logical Operators 

        boolean p = true; 

        boolean q = false; 

        boolean logicalAnd = p && q; 

        boolean logicalOr = p || q; 

        boolean logicalNotP = !p; 

        System.out.println("Logical Operators:"); 

        System.out.println("Logical AND: " + logicalAnd); 

        System.out.println("Logical OR: " + logicalOr); 

        System.out.println("Logical NOT for P: " + logicalNotP); 

        System.out.println(); 

       // Unary Operators 

        int num = 5; 

        int unaryPlus = +num; 

        int unaryMinus = -num; 

        boolean isNot = !true; 

        System.out.println("Unary Operators:"); 

        System.out.println("Unary Plus: " + unaryPlus); 

        System.out.println("Unary Minus: " + unaryMinus); 

        System.out.println("Logical NOT: " + isNot); 

        System.out.println(); 

       // Bitwise Operators 

        int num1 = 5; // 00000101 

        int num2 = 3; // 00000011 

        int bitwiseAnd = num1 & num2; // 00000001 -> 1 

        int bitwiseOr = num1 | num2; // 00000111 -> 7 

        int bitwiseXor = num1 ^ num2; // 00000110 -> 6 

        int bitwiseComplementNum1 = ~num1; // 11111010 -> -6 

        System.out.println("Bitwise Operators:"); 

        System.out.println("Bitwise AND: " + bitwiseAnd); 

        System.out.println("Bitwise OR: " + bitwiseOr); 

        System.out.println("Bitwise XOR: " + bitwiseXor); 

        System.out.println("Bitwise Complement of num1: " + bitwiseComplementNum1); 

    } 

} 
